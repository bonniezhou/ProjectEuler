import math

def is_prime(anumber):
    n = int(math.sqrt(anumber))
    prime = 1
    number = prime + 1
    list_primes =[]
    while prime < n:
        for i in list_primes:
            if i > int(math.sqrt(number)):
                prime = number
                if anumber % prime == 0:
                    return False
                list_primes.append(prime)
                number = number + 1
                break
            elif number % i == 0:
                number = number + 1
                break
        else:
            prime = number
            if anumber % prime == 0:
                    return False
            list_primes.append(prime)
            number = number + 1
    return True
    
print is_prime(7109)
print is_prime(109673)
print is_prime(673109)
print is_prime(17)
print is_prime(21)
